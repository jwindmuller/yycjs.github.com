<div class="container-fluid">
	<div class="row-fluid">
	  <div class="span12">
	    <h1>Our Projects</h1>
	    <hr>
	  </div>
	</div>

	{{#projects}}
	<div class="project">
	  <div class="row-fluid">
	    <div class="span12">
	      <h2>{{name}}</h2>
	      <h3>{{description}}</h3>
	    </div>
	  </div>

	  <div class="row-fluid">
	    <div class="description span8">
	      <p>(Project Summary markdown) Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
	      tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam,
	      quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo
	      consequat... <a href="" target="_blank">more</a></p>

	      <ul class="media-list">
	      	{{#html_url}}
	        <li class="media">
	          <i class="icon-github">
	          <a href="{{html_url}}" target="_blank"></i>Github Repo</a>
	        </li>
	        {{/html_url}}
	        {{#homepage}}
	        <li class="media">
	          <i class="icon-home-1"></i>
	          <a href="{{homepage}}" target="_blank">Home Page</a>
	        </li>
	        {{/homepage}}
	      </ul>
	    </div>
	    <div class="span4">
	      <img class="media-object" src="http://placehold.it/200x200">
	    </div>
	  </div>
	</div><!-- ./project -->
	{{/projects}}
</div>