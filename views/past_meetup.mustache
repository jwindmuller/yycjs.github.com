<div class="meetup">
  <div class="row-fluid">
    <div class="span12">
      <h2>{{name}}</h2>
      <h3>{{date}} <a href="{{event_url}}"><i class="icon-meetup"></i></a></h3>
    </div>
  </div>

  <div class="row-fluid">
    <h3>Presenters</h3>
    <ul class="media-list">
      <li class="media">
        <a class="pull-left" href="#">
          <img class="img-circle member-photo" src="http://photos3.meetupstatic.com/photos/member/4/a/e/4/thumb_94459172.jpeg">
        </a>
        Eric Kryski
        <a href="http://twitter.com/ekryski" target="_blank"><i class="icon-twitter"></i></a>
        <a href="http://github.com/ekryski" target="_blank"><i class="icon-github"></i></a>
        <a href="http://erickryski.com" target="_blank"><i class="icon-home-1"></i></a>
      </li>

      <li class="media">
        <a class="pull-left" href="#">
          <img class="img-circle member-photo" src="http://photos3.meetupstatic.com/photos/member/e/7/1/8/thumb_10679160.jpeg">
        </a>
        David Luecke
        <a href="http://twitter.com/daffl" target="_blank"><i class="icon-twitter"></i></a>
        <a href="http://github.com/daffl" target="_blank"><i class="icon-github"></i></a>
        <a href="http://daffl.github.com" target="_blank"><i class="icon-home-1"></i></a>
      </li>
    </ul>
  </div>

  <div class="row-fluid">
    <div class="description span12">
      <h3>Itinerary</h3>
      {{{description}}}
    </div>
  </div>
</div> <!-- ./meetup -->